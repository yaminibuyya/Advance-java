  	<url-pattern>/eUpdate</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>EmployeeDeleteController</servlet-name>
  	<url-pattern>/delete</url-pattern>
  </servlet-mapping>
 

 
  
