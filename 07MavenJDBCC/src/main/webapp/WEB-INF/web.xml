  </servlet>
  <servlet>
  	<servlet-name>UPDATINGController</servlet-name>
  	<display-name>UPDATINGController</display-name>
  	<description></description>
  	<servlet-class>com.ciq.controller.UPDATINGController</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>updateFINDall</servlet-name>
  	<display-name>updateFINDall</display-name>
  	<description></description>
  	<servlet-class>com.ciq.controller.updateFINDall</servlet-class>
  </servlet>
  <servlet>
