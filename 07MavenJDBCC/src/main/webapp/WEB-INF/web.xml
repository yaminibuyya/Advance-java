  	<display-name>GetByIdController</display-name>
  	<description></description>
  	<servlet-class>com.ciq.controller.GetByIdController</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>EmployeeUpdateController</servlet-name>
  	<display-name>EmployeeUpdateController</display-name>
  	<description></description>
  	<servlet-class>com.ciq.controller.EmployeeUpdateController</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>EmployeeDeleteController</servlet-name>
  	<display-name>EmployeeDeleteController</display-name>
  	<description></description>
  	<servlet-class>com.ciq.controller.EmployeeDeleteController</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>EmployeeFindAllController</servlet-name>
  	<url-pattern>/FindAll</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>EmployeeSaveController</servlet-name>
  	<url-pattern>/Save</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>UPDATINGController</servlet-name>
  	<url-pattern>/update</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>updateFINDall</servlet-name>
  	<url-pattern>/updateFINDall</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>GetByIdController</servlet-name>
  	<url-pattern>/getbyid</url-pattern>
